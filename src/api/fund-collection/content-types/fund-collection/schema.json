{
  "kind": "collectionType",
  "collectionName": "fund_collections",
  "info": {
    "singularName": "fund-collection",
    "pluralName": "fund-collections",
    "displayName": "Fund Collection",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "organization": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::organization.organization",
      "inversedBy": "fund_collections"
    },
    "title": {
      "type": "string",
      "required": true
    },
    "totalGoal": {
      "type": "integer"
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "fund_collections"
    },
    "collectedAmount": {
      "type": "decimal"
    },
    "description": {
      "type": "blocks"
    },
    "requisites": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::requisite.requisite",
      "inversedBy": "fund_collections"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "documents": {
      "allowedTypes": [
        "files"
      ],
      "type": "media",
      "multiple": true
    }
  }
}
